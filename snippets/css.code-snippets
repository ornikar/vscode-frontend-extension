{
  "Theme shortcut for styled-components": {
    "scope": "css",
    "prefix": "sctheme",
    "body": ["${({ theme }) => theme.$1}"]
  },
  "Theme's spacing shortcut for styled-components": {
    "scope": "css",
    "prefix": "scspacing",
    "body": ["${({ theme }) => theme.kitt.spacing * $1}px"]
  },
  "Prop shortcut for styled-components": {
    "scope": "css",
    "prefix": "scprop",
    "body": ["${({ $1 }) => $2}"]
  }
}
