{
  "name": "vscode-ornikar-extension",
  "displayName": "Ornikar Extension",
  "publisher": "ornikar",
  "repository": "ornikar/vscode-frontend-extension",
  "description": "Snippets for Ornikar Frontend Developers",
  "version": "1.5.1",
  "prettier": "@ornikar/prettier-config",
  "license": "MIT",
  "icon": "images/logo-512x512.png",
  "engines": {
    "vscode": "^1.55.0",
    "node": ">=16.10.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/modules.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/modules.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/react.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/kitt.code-snippets"
      }
    ]
  },
  "scripts": {
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "$(yarn bin ornikar-prettier-check)",
    "lint:prettier:fix": "$(yarn bin ornikar-prettier-fix)"
  },
  "devDependencies": {
    "@ornikar/prettier-config": "6.0.6",
    "@ornikar/repo-config": "10.4.3",
    "prettier": "2.8.8",
    "vsce": "2.7.0"
  },
  "packageManager": "yarn@3.6.0"
}
